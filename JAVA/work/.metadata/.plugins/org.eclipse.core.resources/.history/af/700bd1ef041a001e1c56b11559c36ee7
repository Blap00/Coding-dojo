package com.CodingDojo.GuardiaZoologicoP1;

public class GorillaTest{
	public static void main(String[] args) {
		Gorilla gor= new Gorilla(50);
		gor.energ.displayEnergy();
		gor.throwSomething();
		gor.energ.displayEnergy();
		gor.throwSomething();
		gor.energ.displayEnergy();
		gor.throwSomething();
		gor.energ.displayEnergy();
		gor.eatBananas();
		gor.energ.displayEnergy();
		gor.eatBananas();
		gor.energ.displayEnergy();
		gor.climb();
		gor.energ.displayEnergy();
	}
}